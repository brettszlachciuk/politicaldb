<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label fx:id="label" layoutX="206.0" layoutY="21.0"
			prefHeight="43.0" prefWidth="200.0"
			text="Select the action you want to perform" />
		<Button fx:id="searchPol" layoutX="87.0" layoutY="103.0"
			mnemonicParsing="false" onAction="#openSearchpol" prefHeight="26.0"
			prefWidth="106.0" text="Search a Politician" />
		<Button fx:id="searchbill" layoutX="87.0" layoutY="181.0"
			mnemonicParsing="false" onAction="#openSearchbill" prefHeight="26.0"
			prefWidth="106.0" text="Search a Bill" />
		<Button fx:id="addpol" layoutX="87.0" layoutY="259.0"
			mnemonicParsing="false" onAction="#openAddpol" prefHeight="26.0"
			prefWidth="106.0" text="Add a Politician" />
		<Button fx:id="delete" layoutX="87.0" layoutY="346.0"
			mnemonicParsing="false" onAction="#openDelete" prefHeight="26.0"
			prefWidth="106.0" text="Delete a Politician" />
	</children>
</AnchorPane>
